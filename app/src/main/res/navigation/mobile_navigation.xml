<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.ersaal.core.ui.fragments.HomeFragment"
        android:label="@string/dash"
        tools:layout="@layout/fragment_home" />
    <fragment
        android:id="@+id/navigation_scheduler"
        android:name="com.example.ersaal.core.ui.fragments.SchedularFragment"
        android:label="@string/dash"
        tools:layout="@layout/fragment_schedular" />
    <fragment
        android:id="@+id/navigation_recents"
        android:name="com.example.ersaal.core.ui.fragments.RecentsFragment"
        android:label="@string/dash"
        tools:layout="@layout/fragment_recents" />
    <fragment
        android:id="@+id/navigation_settings"
        android:name="com.example.ersaal.core.ui.fragments.SettingsFragment"
        android:label="@string/dash"
        tools:layout="@layout/fragment_settings" />



</navigation>